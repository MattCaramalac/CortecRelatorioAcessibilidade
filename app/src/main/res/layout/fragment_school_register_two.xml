            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/top_margins"
            android:text="@string/ao"
            android:textColor="@color/black"
            android:textSize="@dimen/section_header"
            app:layout_constraintBottom_toBottomOf="@+id/elem_middle_school_first_level_field"
            app:layout_constraintEnd_toStartOf="@id/elem_middle_school_last_level_field"
