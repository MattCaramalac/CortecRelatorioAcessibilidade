            android:layout_width="60dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/maternal_levels_text_to"
            app:layout_constraintBottom_toBottomOf="@id/maternal_levels_text_to"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/top_margins"
            android:enabled="false">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/maternal_last_level_value"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:maxLength="@integer/min_length_grade"/>

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.checkbox.MaterialCheckBox
            android:id="@+id/checkbox_pre_school_classes"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:text="@string/check_box_pre_school"
            android:textSize="18sp"
            android:layout_marginTop="@dimen/top_margins"
            app:layout_constraintTop_toBottomOf="@id/checkbox_maternal_classes"
            app:layout_constraintStart_toStartOf="parent"/>

        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:id="@+id/pre_school_first_level_field"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/checkbox_pre_school_classes"
            app:layout_constraintBottom_toBottomOf="@id/checkbox_pre_school_classes"
            app:layout_constraintEnd_toStartOf="@id/pre_school_levels_text_to"
            android:layout_marginEnd="@dimen/top_margins"
            android:enabled="false">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/pre_school_first_level_value"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:maxLength="@integer/min_length_grade"/>

        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:id="@+id/pre_school_levels_text_to"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/top_margins"
            android:text="@string/ao"
            android:textColor="@color/black"
            android:textSize="@dimen/section_header"
            app:layout_constraintBottom_toBottomOf="@+id/pre_school_first_level_field"
            app:layout_constraintEnd_toStartOf="@id/pre_school_last_level_field"
            app:layout_constraintTop_toTopOf="@+id/pre_school_first_level_field" />

        <com.google.android.material.textfield.TextInputLayout
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:id="@+id/pre_school_last_level_field"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/pre_school_levels_text_to"
            app:layout_constraintBottom_toBottomOf="@id/pre_school_levels_text_to"
